{
  "name": "pcgf/autodrafts",
  "type": "phpbb-extension",
  "description": "An extension that allows the user to save and load drafts. Also drafts will be created automatically every 20 seconds. The extension works offline all the way, so no communication with the server is needed.",
  "homepage": "https://github.com/MarkusWME/phpBB---Auto-Drafts",
  "version": "1.0.0",
  "time": "2016-04-19",
  "keywords": [
    "draft",
    "drafts",
    "quick draft",
    "auto draft"
  ],
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "MarkusWME",
      "homepage": "https://github.com/MarkusWME",
      "email": "markuswme@pcgamingfreaks.at",
      "role": "Lead Developer"
    }
  ],
  "require": {
    "php": ">=5.3.3"
  },
  "extra": {
    "display-name": "Auto Drafts",
    "soft-require": {
      "phpbb/phpbb": ">=3.1.*"
    },
    "version-check": {
      "host": "api.pcgf.at",
      "directory": "/phpbb",
      "filename": "autodrafts.json"
    }
  }
}